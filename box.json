{
    "chmod": "0755",
    "directories": [
        "."
    ],
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests"],
            "in": "vendor"
        }
    ],
    "compression": "GZ",
    "git-version": "package_version",
    "main":        "bin/phpdoc-checker",
    "output":      "phpdoc-checker.phar",
    "stub":        true
}
